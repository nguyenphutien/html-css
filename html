    <table border="1" cellspacing="0" cellpadding="5">
    <!-- border: độ đậm của viền-->
    <!-- cellspacing: khoảng cách giữa viền trên và viền dưới-->
    <!-- cellpadding: khoảng cách của viền với nội dung-->

        <thead>
            <tr>
                <td>1</td>
                <td>2</td>
                <td>3</td>
                <td>4</td>
                <td>5</td>
            </tr>
        </thead>
    
        <tbody>
            <tr> <!--tr: thêm hàng-->
                <td colspan="2">hàng 1 cột 1 và hàng 1 cột 2</td> <!--td: thêm cột-->
                <td>hàng 1 cột 2</td>
                <td rowspan="2">hàng 1 cột 3</td>
                <td>hàng 1 cột 4</td>
            </tr>
            <tr>
                <td>hàng 2 cột 1</td>
                <td>hàng 2 cột 2</td>
                <td>hàng 2 cột 3</td>
                <td>hàng 2 cột 4</td>
            </tr>
        </tbody>

